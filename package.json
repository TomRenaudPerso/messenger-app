{
  "name": "messenger_app",
  "version": "1.0.0",
  "description": "Messenger App",
  "main": "client/src/index.js",
  "private": true,
  "scripts": {
    "client": "cd client && parcel --no-autoinstall --no-source-maps -p 3000 src/entry.js",
    "server": "cd server && node ./entry.cjs",
    "dev": "concurrently -k \"yarn server\" \"yarn client\" \"yarn test:watch\"",
    "build": "cd client && parcel build src/entry.js --no-source-maps --out-dir ../public/bundle",
    "start": "yarn server",
    "prettify": "prettier --write 'client/src/**/*.js'",
    "test": "jest",
    "test:watch": "jest --notify --watchAll"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@hapi/joi": "^17.0.2",
    "abortcontroller-polyfill": "^1.4.0",
    "antd": "^4.7.0",
    "child_process": "^1.0.2",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.2",
    "cookie-parser": "~1.4.4",
    "date-fns": "^2.23.0",
    "debug": "~2.6.9",
    "downloadjs": "^1.4.7",
    "es6-object-assign": "^1.1.0",
    "es6-shim": "^0.35.5",
    "es6-symbol": "^3.1.3",
    "esm": "^3.2.25",
    "express": "~4.16.1",
    "express-actuator": "^1.3.0",
    "express-handlebars": "^3.1.0",
    "express-sslify": "^1.2.0",
    "fs": "^0.0.1-security",
    "http-errors": "~1.6.3",
    "http-proxy-middleware": "^0.20.0",
    "ioredis": "^4.17.3",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "morgan": "~1.9.1",
    "node-uuid": "^1.4.8",
    "parcel-bundler": "^1.12.4",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "querystring": "^0.2.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "reselect": "^4.0.0",
    "underscore": "^1.9.1",
    "url": "^0.11.0",
    "util": "^0.12.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "jest": "^27.0.6",
    "less": "^3.10.3",
    "node-notifier": "^10.0.0",
    "prettier": "1.19.1",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.25.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom"
  }
}
